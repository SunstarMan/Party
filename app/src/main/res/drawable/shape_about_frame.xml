<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape>
            <stroke
                android:width="1dp"
                android:color="#ff4400"
                android:dashWidth="6dp"
                android:dashGap="4dp" />

            <corners android:radius="16dp" />
        </shape>
    </item>

</layer-list>